{
  "name": "akipe/kif",
  "license": "GPLv3",
  "autoload": {
    "psr-4": {
      "Akipe\\Kif\\": "src/",
      "Akipe\\Lib\\": "lib/"
    }
  },
  "authors": [
    {
      "name": "Julien Milletre Akipe",
      "email": "code.julien@milletre.fr"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "*"
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests",
    "example": "php example/app.php",
    "phpstan": "vendor/bin/phpstan analyse src tests --level 9",
    "check-standard": "vendor/bin/phpcs --standard=PSR12 ./src ./tests",
    "fix-standard": "vendor/bin/phpcbf --standard=PSR12 ./src ./tests"
  },
  "require": {
    "php": "^8.3",
    "ext-intl": "^8.3",
    "ext-xml": "^8.3",
    "symfony/dotenv": "^7.0"
  }
}
